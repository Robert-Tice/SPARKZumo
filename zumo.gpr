project Zumo is

   for Source_Dirs use (".", "src");
   for Object_Dir use "obj";
   for Languages use ("Ada");
   for Target use "c";

   package Ide is
      for Vcs_Kind use "Git";
   end Ide;

   package Compiler is
      for Switches ("sparkduino.ads") use ("-gnatd.5");
      for Switches ("c") use ("--no-complete-output");
   end Compiler;

   package Linker is
      for Switches ("ada") use ("-g");
   end Linker;

end Zumo;

